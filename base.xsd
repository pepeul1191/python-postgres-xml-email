<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="data">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="worker">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="names" type="xs:string" />
              <xs:element name="last_names" type="xs:string" />
              <xs:element name="email" type="xs:unsignedInt" />
              <xs:element name="phone">
                <xs:complexType>
                  <xs:attribute name="whastapp" type="xs:string" use="required" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="id" type="xs:unsignedByte" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>